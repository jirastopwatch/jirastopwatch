<configuration>
    <configSections>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="StopWatch.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
        </sectionGroup>
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
    <userSettings>
        <StopWatch.Properties.Settings>
            <setting name="IssueCount" serializeAs="String">
                <value>6</value>
            </setting>
            <setting name="PersistedIssues" serializeAs="String">
                <value />
            </setting>
            <setting name="AlwaysOnTop" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="Username" serializeAs="String">
                <value />
            </setting>
            <setting name="ApiToken" serializeAs="String">
                <value />
            </setting>
            <setting name="JiraBaseUrl" serializeAs="String">
                <value>http://myjiraserver.local/</value>
            </setting>
            <setting name="FirstRun" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="SaveTimerState" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="CurrentFilter" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="MinimizeToTray" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="PauseOnSessionLock" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="AllowMultipleTimers" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="PostWorklogComment" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="UpgradeRequired" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="StartTransitions" serializeAs="String">
                <value />
            </setting>
            <setting name="LoggingEnabled" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="CheckForUpdate" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="IncludeProjectName" serializeAs="String">
                <value>False</value>
            </setting>
        </StopWatch.Properties.Settings>
    </userSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Runtime.CompilerServices.Unsafe" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
